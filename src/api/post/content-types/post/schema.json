{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Detail": {
      "type": "richtext"
    },
    "Thumb": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::post.post",
      "inversedBy": "posts"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "author"
    }
  }
}
